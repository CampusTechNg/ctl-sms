{
  "name": "ctl-sms-visitors",
  "version": "1.0.0",
  "description": "Campus Technologies Limited Student Management System - Visitors' Module",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "*",
    "express": "*",
    "express-handlebars": "^3.0.0"
  },
  "bolt": {
    "displayName": "Visitors",
    "main": "bolt-server.js",
    "index": "/",
    "system": true,
    "extensions": {
      "/visitors": "/",
      "/visitor": "/"
    },
    "files": {
      "icon": "assets/pages/img/icon.png"
    },
    "hooks": {
      "bolt/app-starting": "/app-starting",
      "bolt/app-collection-inserted": "/hooks/bolt/visits-modified",
      "bolt/app-collection-removed": "/hooks/bolt/visits-modified",
      "bolt/app-collection-updated": "/hooks/bolt/visits-modified",
      "bolt/app-collection-replaced": "/hooks/bolt/visits-modified"
    },
    "order": 3,
    "public": ["assets"],
    "tags": ["visitors", "visitor", "ctl", "ctl-sms", "ctl-sms-visitors", "ctl-sms-plugins"],
    "collections": {
      "visitors": "*",
      "visits": "*"
    },
    "dependencies": {
      "ctl-sms-home": "*"
    }
  },
  "author": "Campus Technologies Limited",
  "license": "ISC"
}
