{
  "name": "ctl-sms-scores",
  "version": "1.0.0",
  "description": "Campus Technologies Limited Student Management System - Scores Module",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "*",
    "express": "*",
    "express-handlebars": "^3.0.0"
  },
  "bolt": {
    "displayName": "Scores",
    "main": "bolt-server.js",
    "index": "/",
    "system": true,
    "extensions": {
      "/scores": "/",
      "score": "/"
    },
    "files": {
      "icon": "assets/pages/img/icon.png",
      "icon-md": "assets/pages/img/icon-md.png"
    },
    "order": 2,
    "public": ["assets"],
    "tags": ["scores", "score", "ctl", "ctl-sms", "ctl-sms-scores", "ctl-sms-score", "ctl-sms-plugins"],
    "hooks": {
      "bolt/app-starting": "/hook/bolt/app-starting"
    },
    "permissions": {
      "edit-scores": {
        "displayName": "Permission to edit scores",
        "description": "This grants the permission to edit scores"
      },
      "view-scores": {
        "displayName": "Permission to view scores",
        "description": "This grants the permission to view scores"
      }
    },
    "dependencies": {
      "metronic-for-bolt": "*",
      "ctl-sms-home": "*",
      "ctl-sms-school-admin": "*"
    }
  },
  "author": "Campus Technologies Limited",
  "license": "ISC"
}
